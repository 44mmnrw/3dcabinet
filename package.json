{
  "name": "3dcabinet",
  "version": "1.0.0",
  "description": "Веб-приложение для виртуального проектирования размещения оборудования в серверном шкафу.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently \"npm run dev\" \"php artisan serve\" -n \"VITE,PHP\" -c \"cyan,green\"",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && echo 'Production build готов! Теперь можно коммитить.'",
    "build:icons": "node build-icons.mjs",
    "icons:watch": "chokidar \"public/assets/icons/**/*.svg\" -c \"node build-icons.mjs\"",
    "icons:dev": "npm run build:icons && npm run icons:watch"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^25.0.0",
    "camera-controls": "^3.1.0",
    "cannon-es": "^0.20.0",
    "draco3d": "^1.5.7",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "ktx-parse": "^1.1.0",
    "lil-gui": "^0.21.0",
    "postprocessing": "^6.38.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "stats.js": "^0.17.0",
    "sweetalert2": "^11.26.3",
    "three": "^0.181.1",
    "three-mesh-bvh": "^0.9.2",
    "troika-three-text": "^0.52.4"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.1",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "glob": "^11.0.0",
    "laravel-vite-plugin": "^2.0.1",
    "svgstore": "^3.0.1",
    "vite": "^7.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/44mmnrw/3dcabinet.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/44mmnrw/3dcabinet/issues"
  },
  "homepage": "https://github.com/44mmnrw/3dcabinet#readme",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  }
}
