# --- Laravel caches & storage ---
# Ignore compiled/cache files generated at runtime
/bootstrap/cache/*.php
!/bootstrap/cache/.gitignore

# Ignore storage runtime data (logs, sessions, views, cache)
/storage/logs
/storage/framework/*
!/storage/framework/.gitignore
/storage/framework/cache/**
/storage/framework/sessions
/storage/framework/views
/storage/framework/testing

# --- Environment overrides ---
.env.local
.env.*.local
.env.testing

# --- NPM/Yarn/Pnpm debug logs ---
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
lerna-debug.log*

# --- Code coverage ---
/coverage

# Optional: ignore generated SVG sprite if you rebuild it in CI
# /public/assets/sprite/sprite.svg
